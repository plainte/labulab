<!-- /templates/collection-services.liquid -->
<div class="grid">

  <header class="section-header text-center">
    <h2>{{ collection.title }}</h2>
    <hr class="hr--small">
  </header>

  <div class="wave lazyload">
    <img
    {% if collection.title contains "PRIVÁT" %}
      src="{{ 'wave.png' | asset_url }}"
    {% else %}
      src="{{ 'direct-wave.png' | asset_url }}"
    {% endif %}
      loading="lazy"
      alt="project picture"
      data-sizes="auto"
      class="image-absolute">
  </div>
  <div class="simple-i-design">
    <div class="simple-i-design-content" id="intro">
    </div>
  </div>
  <div class="wave lazyload">
    <img 
    {% if collection.title contains "PRIVÁT" %}
      src="{{ 'bottom-wave.png' | asset_url }}"
    {% else %}
      src="{{ 'bottom-direct-wave.png' | asset_url }}"
    {% endif %}
      loading="lazy"
      alt="project picture"
      data-sizes="auto"
      class="image-absolute">
  </div>
  <div class=px50-div></div>
  <div class="i-column-with-top-bottom-margin">
    <div class="i-card-colored-border" id="info">
    <h5 style="text-transform: inherit">#miértmi</h5>
    <ul>
    </ul>
    </div>
  </div>
  <div class=px50-div></div>
  <div>
    <h5 class="text-center" id="process-intro">
    </h5>
  </div>
  <div class=px50-div id="process-begin"></div>
  <!-- process rendered from json -->
  <div class=px50-div id="process-end"></div>
  <div class="i-column-with-top-bottom-margin">
    <div class="i-card-colored-border" id="extra-details">
    <h4 style="text-transform: inherit"></h4>
    <ul>
    </ul>
    </div>
  </div>
  <div class=px50-div></div>
  <div class="wave lazyload">
    <img 
    {% if collection.title contains "PRIVÁT" %}
      src="{{ 'wave.png' | asset_url }}"
    {% else %}
      src="{{ 'direct-wave.png' | asset_url }}"
    {% endif %}
    loading="lazy"
    alt="project picture"
    data-sizes="auto"
    class="image-absolute">
  </div>
  <div class="simple-i-design">
    <div class="i-credit-row">
      <div class="i-credit-column">
      </div>
      <div class="i-credit-column">
      </div>
    </div>
  </div>
  <div class="wave lazyload">
    <img
    {% if collection.title contains "PRIVÁT" %}
      src="{{ 'bottom-wave.png' | asset_url }}"
    {% else %}
      src="{{ 'bottom-direct-wave.png' | asset_url }}"
    {% endif %}
      loading="lazy"
      alt="project picture"
      data-sizes="auto"
      class="image-absolute">
  </div>
</div>

